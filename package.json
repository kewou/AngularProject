{
  "name": "gestion-loyer",
  "version": "2.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:debug": "ng serve --configuration=development --port=4200 --host=0.0.0.0 --disable-host-check",
    "start:dev": "ng serve --configuration=development --port=4200 --open",
    "start:prod": "ng serve --configuration=production --port=4200",
    "build": "ng build",
    "build:debug": "ng build --configuration=development --source-map=true",
    "build:prod": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^15.2.9",
